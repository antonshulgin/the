{
  "name": "the.js",
  "version": "0.0.3",
  "description": "Type-checking library for Javascript",
  "main": "src/the.js",
  "scripts": {
    "start": "npm run build",
    "build": "npm run reset-dist && npm run uglify",
    "reset-dist": "rm -rf ./dist/ && mkdir ./dist/",
    "uglify": "./node_modules/uglifyjs/bin/uglifyjs ./src/the.js --compress --mangle --output=./dist/the.min.js",
		"test": "npm run build && ./node_modules/mocha/bin/mocha ./test/everything.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonshulgin/the.git"
  },
  "keywords": [
    "the",
    "type-checking",
    "types",
    "check",
    "js"
  ],
  "author": "Anton Shulgin <anton.shulgin@gmx.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antonshulgin/the/issues"
  },
  "homepage": "https://github.com/antonshulgin/the#readme",
  "devDependencies": {
    "mocha": "^3.0.2",
    "uglifyjs": "^2.4.10"
  }
}
